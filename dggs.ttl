# baseURI: http://linked.data.gov.au/def/dggs
# imports: http://linked.data.gov.au/def/loci
# prefix: dggs

@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dggs: <http://linked.data.gov.au/def/dggs#> .
@prefix loci: <http://linked.data.gov.au/def/loci#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wgs84: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://linked.data.gov.au/def/dggs>
  a owl:Ontology ;
  dcterms:created "2020-02-05"^^xsd:date ;
  dcterms:creator "<a href='https://orcid.org/0000-0002-3884-3420'>Simon J D Cox, CSIRO</a>"^^rdf:HTML ;
  dcterms:modified "2020-02-05"^^xsd:date ;
  rdfs:comment """An ontology to allow DGGS objects to be linked into Loc-I. 
  Two classes are defined: a DGGS Geometry is composed of one or more DGGS Cells. 
  A DGGS Geometry is a kind of geometry that may be the object of the hasGeometry property of a Loc-I Feature. """ ;
  rdfs:label "DGGS Ontology" ;
  rdfs:seeAlso <http://www.opengis.net/doc/AS/dggs/1.0> ;
  rdfs:seeAlso <https://github.com/GeoscienceAustralia/AusPIX_DGGS> ;
  owl:imports <http://linked.data.gov.au/def/loci> ;
.
dggs:Cell
  a owl:Class ;
  rdfs:comment """DGGS cell, for example from <a href='https://github.com/GeoscienceAustralia/AusPIX-DGGS-dataset'>AusPIX</a>

Individual cells should be refered to using an IRI, typically a call to the AusPIX service."""^^rdf:HTML ;
  rdfs:label "DGGS Cell" ;
.
dggs:Geometry
  a owl:Class ;
  rdfs:comment "A DGGS Geometry is composed of a set of DGGS Cells" ;
  rdfs:label "DGGS Geometry" ;
  rdfs:subClassOf <http://www.opengis.net/ont/geosparql#Geometry> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom dggs:Cell ;
      owl:onProperty rdfs:member ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty rdfs:member ;
    ] ;
.
